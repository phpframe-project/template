{
    "name": "phpframe-project/template",
    "description": "The template with PHPFrame Framework",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-pdo": "*",
        "phpframe-project/phpframe": "^0.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "mockery/mockery": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "secure-http": false
    },
    "prefer-stable": true
}
